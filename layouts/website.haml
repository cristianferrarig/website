!!!
%html
  %head
    = partial "meta/base"
    = partial "meta/social"
    = partial "meta/mobile"
    = partial "meta/brand"
    = partial "meta/tricks"

    = stylesheet_link_tag "website"

    = partial "utils/oldie"

  - if content_for? :assets
    = yield :assets

  %body{ class: "website #{page_classes}" }
    =# render "utils/typekit"

    - if build?
      = partial "utils/tagmanager"

    .container
      .logo

      = yield

    -# Placed at the end of the document so the pages load faster
    %script{ src: "https://code.jquery.com/jquery-1.10.2.min.js" }
    =# javascript_include_tag  "jquery"
    = javascript_include_tag  "app"

    =# render "utils/analytics"
